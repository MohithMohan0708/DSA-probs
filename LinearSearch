#include <stdio.h>

int main()
{
    int i,key,n;
    printf("enter the number of elements\n");
    scanf("%d",&n);
    int a[n];
    printf("enter the elements\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter the key element\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(a[i]==key)
        {
            printf("%d is found at %d position\n",a[i],i+1);
            break;
        }
        if(i==(n-1))
        {
            printf("element not found");
        }
    }
    return 0;
}
